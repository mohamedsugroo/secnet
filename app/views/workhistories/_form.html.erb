<%= simple_form_for(@workhistory) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_name %>
    <%= f.input :your_role %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :note %>
    <%#= f.association :user %>
    <%#= f.association :contractor %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
